@article{bharadwaj_overview_2021,
	title = {An Overview on Biomaterials and Its Applications in Medical Science},
	volume = {1116},
	issn = {1757-8981, 1757-899X},
	url = {https://iopscience.iop.org/article/10.1088/1757-899X/1116/1/012178},
	doi = {10.1088/1757-899X/1116/1/012178},
	abstract = {Biomaterials have been used tremendously in medical science over last many decades. In human body a number of body tissues like teeth, ligament, bones tendons and others have been successfully replaced by these biomaterials. In present scenario the various applications of these biomaterials are still awaited. The most important challenge in using these biomaterials is immune rejection because in current situation the lifetime implants and bone replacement must required biocompatibility along with the biological and mechanical characteristics of the biomaterial used. Till now a number of biomaterials have been discovered and due to their biocompatibility and biodegradability, these are biomaterials are employed significantly in biotherapy and medical science. On the basis of the source, these biomaterials can be grouped into two categories namely natural or synthetic polymers, henceforth they have paid much consideration.},
	pages = {012178},
	number = {1},
	journaltitle = {{IOP} Conference Series: Materials Science and Engineering},
	shortjournal = {{IOP} Conf. Ser.: Mater. Sci. Eng.},
