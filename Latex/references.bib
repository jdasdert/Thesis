@article{menicucci_toothimplant_2002,
	title = {Tooth–implant connection: some biomechanical aspects based on finite element analyses},
	volume = {13},
	rights = {http://onlinelibrary.wiley.com/{termsAndConditions}\#vor},
	issn = {0905-7161, 1600-0501},
	url = {https://onlinelibrary.wiley.com/doi/10.1034/j.1600-0501.2002.130315.x},
	doi = {10.1034/j.1600-0501.2002.130315.x},
	shorttitle = {Tooth–implant connection},
	abstract = {This study investigated, with the use of two- and three-dimensional ﬁnite element analysis, the peri-implant stress that occurred during loading of a tooth that was rigidly connected to a distally placed implant. A ﬁxed bond between the bone and the implant was assumed, while the periodontal ligament was represented by means of three-dimensional nonlinear visco-elastic spring elements. Two different loading conditions were compared in the study: i) an axially directed static load of 50 kg that was applied to the tooth for 10 s, and ii) a transitional load of 50 kg that was applied for 5 milliseconds. Load duration appeared to have a greater inﬂuence than load intensity on the stress distribution in the bone around an implant and a rigidly connected tooth. Static load is, therefore, potentially more harmful for peri-implant bone than transitional load. The periodontal ligament seems to play a key role in the stress distribution between a tooth and a rigidly connected implant.},
	pages = {334--341},
	number = {3},
	journaltitle = {Clinical Oral Implants Research},
	shortjournal = {Clinical Oral Implants Res},
	author = {Menicucci, Giulio and Mossolov, Alexei and Mozzati, Marco and Lorenzetti, Massimo and Preti, Giulio},
